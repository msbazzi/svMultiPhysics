#----------------------------------------------------------------
# General simulation parameters
#
Continue previous simulation: 1
Number of spatial dimensions: 3
Number of time steps: 32000
Time step size: 0.00025000 % 4000 timesteps per cycle, 60 bpm, cycle 1sec
Spectral radius of infinite time step: 0.50
Searched file name to trigger stop: STOP_SIM

# Initialize the simulation from the NH run with slightly different material parameters
# Simulation initialization file path:  result_ini.vtu

Save results to VTK format: 1
Name prefix of saved VTK files: result
Increment in saving VTK files: 50
Start saving after time step: 28000

Increment in saving restart files: 1000
Convert BIN to VTK format: 0

Verbose: 1
Warning: 0
Debug: 0

#----------------------------------------------------------------
# Mesh data
#
Add mesh: fluid {
   Mesh file path :                                        ../mesh_fluid/mesh-complete.mesh.vtu
   Add face: fluid_wall                 { Face file path : ../mesh_fluid/mesh-surfaces/surface.vtp }
   Add face: fluid_inlet                { Face file path : ../mesh_fluid/mesh-surfaces/inlet.vtp}
   Add face: fluid_BCT                  { Face file path : ../mesh_fluid/mesh-surfaces/branch_1.vtp}
   Add face: fluid_LCC                  { Face file path : ../mesh_fluid/mesh-surfaces/branch_2.vtp}
   Add face: fluid_LSA                  { Face file path : ../mesh_fluid/mesh-surfaces/branch_3.vtp}
   Add face: fluid_outlet               { Face file path : ../mesh_fluid/mesh-surfaces/outlet.vtp}
   Domain: 1
}

Add mesh: wall {
   Mesh file path :                            ../mesh_solid/mesh-complete.mesh.vtu
   Add face : wall_inner    { Face file path : ../mesh_solid/mesh-surfaces/inner_surface.vtp }
   Add face : wall_outer    { Face file path : ../mesh_solid/mesh-surfaces/outer_surface.vtp }
   Add face : wall_inlet    { Face file path : ../mesh_solid/mesh-surfaces/inlet.vtp }
   Add face : wall_BCT      { Face file path : ../mesh_solid/mesh-surfaces/branch_1.vtp }
   Add face : wall_LCC_LSA  { Face file path : ../mesh_solid/mesh-surfaces/branch_2.vtp }
   Add face : wall_outlet   { Face file path : ../mesh_solid/mesh-surfaces/outlet.vtp }
   Domain: 2
   Prestress file path:   ./prestress.vtu
}

#----------------------------------------------------------------
# Projections
Add projection: wall_inner {
 Project from face: fluid_wall
  Projection tolerance: -1
  }

#----------------------------------------------------------------
# Equations
Add equation: FSI {

   Coupled: 1
   Min iterations: 1
   Max iterations: 10
   Tolerance: 1e-4

   Domain: 1 {
      Equation: fluid
      Density: 0.00106 #  g/mm^3 [1.1 g/cm^3]  
      Viscosity: Constant {Value: 0.004}  #  g/mm s  [ 0.04 g / cm s], 4.0 cP
      Backflow stabilization coefficient: 0.3
   }
#
# true lumen outer wall
# TBAD 2009: 
# Parameters from KB 2023-06-29
# true lumen outer wall
Domain: 2 {
      Equation: struct
      Constitutive model: nHK
      Dilational penalty model: ST91
      Density: 0.0012 # [1.2 g/cm^3]
      Elasticity modulus: 600.0e3 # 600 kPa in TL outer wall
      Poisson ratio: 0.49
   }

   Output: Spatial {
      Traction: t
      Stress: t
      Displacement: t
      Velocity: t
      Pressure: t
      WSS: t
   }

   Output: Alias {
      Displacement: FS_Displacement   ## or Fluid_displacement
   }

   Output: Boundary_integral {
      Pressure: t
      Absolute_velocity: t
   }

################ LUMEN BC
   
   Add BC: fluid_inlet {
      Type: Dirichlet
      Time dependence: Unsteady
      Temporal values file path: flowrate.flow
      Profile: Parabolic
      Zero out perimeter: 1
      Impose flux: 1
   }


   Add BC: fluid_BCT {
      Type: Neu
      Time dependence: RCR
      RCR values: ( 0.114728136, 1.081318324, 1.032553224)
      Distal pressure: 0.0
   }

   Add BC: fluid_LCC {
      Type: Neu
      Time dependence: RCR
      RCR values: ( 0.548699781, 0.226093831, 4.938298028)
      Distal pressure: 0.0
   }

   Add BC: fluid_LSA {
      Type: Neu
      Time dependence: RCR
      RCR values: ( 0.315502374, 0.393206663, 2.839521366)
      Distal pressure: 0.0
   }

   Add BC: fluid_outlet {
      Type: Neu
      Time dependence: RCR
      RCR values: ( 0.022455685, 5.524553619, 0.202101165)
      Distal pressure: 0.0
   }

################# ANNULUS BC
# Robin Values: k_s (Stiffness  / displacement), c_s (Damping / vel), p_0 (Value)
   Add BC: wall_outer {
      Type: Robin
      Time dependence: Steady
      Value: 0.0d0
      Stiffness: 1.8D4
      Damping: 3.0D1
   }

   Add BC: wall_inlet { 
           Type: Dirichlet
           Time dependence: Steady
           Value: 0.0d0
           Impose on state variable integral: t
   }

   Add BC: wall_BCT { 
           Type: Dirichlet
           Time dependence: Steady
           Value: 0.0d0
           Impose on state variable integral: t
   }

   Add BC: wall_LCC_LSA { 
           Type: Dirichlet
           Time dependence: Steady
           Value: 0.0d0
           Impose on state variable integral: t
   }

   Add BC: wall_outlet { 
           Type: Dirichlet
           Time dependence: Steady
           Value: 0.0d0
           Impose on state variable integral: t
   }
}

Add equation: mesh {
   Coupled: 1
   Min iterations: 1
   Max iterations: 8
   Tolerance: 1e-4
   Poisson ratio: 0.3

   Output: Spatial {
      Displacement: t
   }

   Output: Alias {
   Displacement: MS_Displacement   ## or Mesh_displacement
   }

   LS type: CG {
      Preconditioner: FSILS
      Tolerance: 1e-3
      Max iterations: 500
   }
}
